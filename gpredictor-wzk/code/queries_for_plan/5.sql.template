select
    n_name,
    sum(ol_amount) as revenue
from
    customer,
    orders,
    order_line,
    stock,
    supplier,
    nation,
    region
where
    c_id = o_c_id
    and c_w_id = o_w_id
    and c_d_id = o_d_id
    and ol_o_id = o_id
    and ol_w_id = o_w_id
    and ol_d_id=o_d_id
    and ol_w_id = s_w_id
    and ol_i_id = s_i_id
    and mod((s_w_id * s_i_id), 10000) = su_suppkey
    and c_nationkey = su_nationkey
    and su_nationkey = n_nationkey
    and n_regionkey = r_regionkey
    and r_name = 'Europe'
    and o_entry_d >= %(begin_date)s
    and o_entry_d < %(end_date)s
    and ol_delivery_d >= %(min_date)s
group by
    n_name
order by
    revenue desc;
